@page
@model IndexModel
@{
    ViewData["Title"] = "Северный ветер";
}
<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width-device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"]</title>
    <link rel="icon" href="~/bundles/favicon.ico?v=2" type="image/x-icon">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Raleway:wght@200&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,200;0,300;0,600;0,700;0,800;1,500&display=swap" rel="stylesheet">
    <title>ООО Северный ветер: Вывоз ЖБО и отходов Симферополь</title>
    <meta name="description" content="Компания, оказывающая услуги по вывозу крупногабаритных и жидких бытовых отходов в Симферополе и Крыму.">
    <meta name="keywords" content="Северный, Ветер, Симферополь, мусор, откачка, ЖБО, ассенизатор, Крым, жидкие, отходы">
</head>
<body>
<div id="app" style="margin: 0; padding: 0;"></div>
<script src="~/bundles/js/chunk-vendors.js" asp-append-version="true"></script>
<script src="~/bundles/js/app.js" asp-append-version="true"></script>
</body>
</html>